{
    "name": "sweet1s/moonshine-filemanager",
    "description": "Moonshine File Manager is a Laravel package that provides a simple way to manage files and folders in your Laravel application.",
    "keywords": [
        "laravel",
        "moonshine",
        "filemanager",
        "medialibrary"
    ],
    "type": "library",
    "homepage": "https://moonshine-laravel.com/",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/sweet1s/moonshine-filemanager/issues",
        "source": "https://github.com/sweet1s/moonshine-filemanager"
    },
    "authors": [
        {
            "name": "Andrei Balan",
            "email": "abalan1510it@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.8",
        "mockery/mockery": "^1.4.4",
        "phpstan/phpstan": "^1.4.7",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0",
        "brianium/paratest": "^6.8",
        "moonshine/moonshine": "^3.0",
        "unisharp/laravel-filemanager": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Sweet1s\\MoonshineFileManager\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sweet1s\\MoonshineFileManager\\Providers\\MoonshineFileManagerServiceProvider"
            ]
        }
    }
}
